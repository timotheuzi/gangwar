{
  "GameState": {
    "description": "Main game state dataclass",
    "fields": {
      "player_name": {"type": "str", "default": ""},
      "gang_name": {"type": "str", "default": ""},
      "money": {"type": "int", "default": 1000},
      "account": {"type": "int", "default": 0},
      "loan": {"type": "int", "default": 0},
      "members": {"type": "int", "default": 1},
      "squidies": {"type": "int", "default": 25},
      "squidies_pistols": {"type": "int", "default": 10},
      "squidies_ar15s": {"type": "int", "default": 5},
      "squidies_bullets": {"type": "int", "default": 100},
      "squidies_grenades": {"type": "int", "default": 20},
      "squidies_missile_launcher": {"type": "int", "default": 2},
      "squidies_missiles": {"type": "int", "default": 10},
      "day": {"type": "int", "default": 1},
      "health": {"type": "int", "default": 30},
      "steps": {"type": "int", "default": 0},
      "max_steps": {"type": "int", "default": 15},
      "current_score": {"type": "int", "default": 0},
      "current_location": {"type": "str", "default": "city"},
      "drug_prices": {
        "type": "dict",
        "default": {
          "weed": 500,
          "crack": 1000,
          "coke": 2000,
          "ice": 1500,
          "percs": 800,
          "pixie_dust": 3000
        }
      },
      "lives": {"type": "int", "default": 3},
      "damage": {"type": "int", "default": 0},
      "flags": {"type": "Flags", "default": "Flags()"},
      "weapons": {"type": "Weapons", "default": "Weapons()"},
      "drugs": {"type": "Drugs", "default": "Drugs()"}
    }
  },
  "Flags": {
    "description": "Game flags dataclass",
    "fields": {
      "has_id": {"type": "bool", "default": false},
      "has_info": {"type": "bool", "default": false}
    }
  },
  "Drugs": {
    "description": "Drugs inventory dataclass",
    "fields": {
      "weed": {"type": "int", "default": 0},
      "crack": {"type": "int", "default": 5},
      "coke": {"type": "int", "default": 0},
      "ice": {"type": "int", "default": 0},
      "percs": {"type": "int", "default": 0},
      "pixie_dust": {"type": "int", "default": 0}
    }
  },
  "Weapons": {
    "description": "Weapons inventory dataclass",
    "fields": {
      "pistols": {"type": "int", "default": 0},
      "bullets": {"type": "int", "default": 0},
      "ar15s": {"type": "int", "default": 0},
      "grenades": {"type": "int", "default": 0},
      "barbed_wire_bat": {"type": "int", "default": 0},
      "missile_launcher": {"type": "int", "default": 0},
      "missiles": {"type": "int", "default": 0},
      "vest": {"type": "int", "default": 0},
      "knife": {"type": "int", "default": 0},
      "ghost_guns": {"type": "int", "default": 0}
    },
    "methods": {
      "can_fight_with_pistol": {
        "description": "Check if player can fight with pistol",
        "logic": "return self.pistols > 0 and self.bullets > 0"
      }
    }
  },
  "HighScore": {
    "description": "High score entry dataclass",
    "fields": {
      "player_name": {"type": "str", "default": ""},
      "gang_name": {"type": "str", "default": ""},
      "score": {"type": "int", "default": 0},
      "money_earned": {"type": "int", "default": 0},
      "days_survived": {"type": "int", "default": 0},
      "gang_wars_won": {"type": "int", "default": 0},
      "fights_won": {"type": "int", "default": 0},
      "date_achieved": {"type": "str", "default": ""}
    }
  }
}
